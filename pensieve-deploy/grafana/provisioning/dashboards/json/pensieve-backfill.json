{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "gridPos": { "h": 3, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "options": {
        "content": "# Pensieve Backfill Dashboard\nMonitoring for `backfill-jsonl` and `backfill-proto` ingestion processes.\n\nMetrics are exported when running with `--metrics-port`.",
        "mode": "markdown"
      },
      "title": "",
      "type": "text"
    },
    {
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 3 },
      "id": 100,
      "title": "Status",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [
            { "options": { "0": { "color": "red", "text": "Stopped" }, "1": { "color": "green", "text": "Running" } }, "type": "value" }
          ],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "red", "value": null }, { "color": "green", "value": 1 }] }
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 4 },
      "id": 2,
      "options": { "colorMode": "background", "graphMode": "none", "justifyMode": "center", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "textMode": "value" },
      "targets": [{ "expr": "backfill_running", "refId": "A" }],
      "title": "Status",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": { "defaults": { "color": { "mode": "palette-classic" }, "unit": "short" } },
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 4 },
      "id": 3,
      "options": { "colorMode": "value", "graphMode": "area", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "textMode": "auto" },
      "targets": [{ "expr": "backfill_events_valid_total", "legendFormat": "Valid Events", "refId": "A" }],
      "title": "Valid Events",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": { "defaults": { "color": { "mode": "palette-classic" }, "unit": "short" } },
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 4 },
      "id": 4,
      "options": { "colorMode": "value", "graphMode": "area", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "textMode": "auto" },
      "targets": [{ "expr": "backfill_events_duplicate_total", "legendFormat": "Duplicates", "refId": "A" }],
      "title": "Duplicates Skipped",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 100 }, { "color": "red", "value": 1000 }] },
          "unit": "short"
        }
      },
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 4 },
      "id": 5,
      "options": { "colorMode": "value", "graphMode": "area", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "textMode": "auto" },
      "targets": [{ "expr": "backfill_events_invalid_total", "legendFormat": "Invalid", "refId": "A" }],
      "title": "Invalid Events",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": { "defaults": { "color": { "mode": "palette-classic" }, "unit": "short" } },
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 4 },
      "id": 8,
      "options": { "colorMode": "value", "graphMode": "area", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "textMode": "auto" },
      "targets": [{ "expr": "backfill_events_written_total", "legendFormat": "Written", "refId": "A" }],
      "title": "Events Written",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": { "defaults": { "color": { "mode": "palette-classic" }, "unit": "ops" } },
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 4 },
      "id": 7,
      "options": { "colorMode": "value", "graphMode": "area", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "textMode": "auto" },
      "targets": [{ "expr": "backfill_events_per_second", "legendFormat": "Events/sec", "refId": "A" }],
      "title": "Processing Rate",
      "type": "stat"
    },
    {
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 8 },
      "id": 101,
      "title": "Throughput",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" }, "custom": { "lineWidth": 1, "fillOpacity": 10 }, "unit": "short" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 9 },
      "id": 10,
      "options": { "legend": { "calcs": ["lastNotNull", "mean"], "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi", "sort": "desc" } },
      "targets": [
        { "expr": "rate(backfill_events_valid_total[1m]) * 60", "legendFormat": "Valid", "refId": "A" },
        { "expr": "rate(backfill_events_written_total[1m]) * 60", "legendFormat": "Written", "refId": "B" },
        { "expr": "rate(backfill_events_duplicate_total[1m]) * 60", "legendFormat": "Duplicate", "refId": "C" },
        { "expr": "rate(backfill_events_invalid_total[1m]) * 60", "legendFormat": "Invalid", "refId": "D" }
      ],
      "title": "Events per Minute (by status)",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" }, "custom": { "lineWidth": 1, "fillOpacity": 10 }, "unit": "Bps" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 9 },
      "id": 11,
      "options": { "legend": { "calcs": ["lastNotNull", "mean"], "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi", "sort": "desc" } },
      "targets": [
        { "expr": "rate(backfill_bytes_total{type=~\"json_raw|proto_compressed\"}[1m])", "legendFormat": "Input ({{type}})", "refId": "A" },
        { "expr": "rate(backfill_bytes_total{type=\"notepack_compressed\"}[1m])", "legendFormat": "Output (notepack gzip)", "refId": "B" }
      ],
      "title": "Throughput (bytes/sec)",
      "type": "timeseries"
    },
    {
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 17 },
      "id": 102,
      "title": "Storage",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": { "color": { "mode": "palette-classic" }, "custom": { "lineWidth": 1, "fillOpacity": 10 }, "unit": "short" }
      },
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 18 },
      "id": 20,
      "options": { "legend": { "calcs": ["lastNotNull"], "displayMode": "table", "placement": "bottom" }, "tooltip": { "mode": "multi" } },
      "targets": [
        { "expr": "backfill_segments_sealed_total", "legendFormat": "Segments Sealed", "refId": "A" }
      ],
      "title": "Segments Sealed Over Time",
      "type": "timeseries"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": { "defaults": { "color": { "mode": "palette-classic" }, "unit": "decbytes" } },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 18 },
      "id": 21,
      "options": { "displayLabels": ["name", "value"], "legend": { "displayMode": "table", "placement": "right", "values": ["value"] }, "pieType": "pie", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "tooltip": { "mode": "single" } },
      "targets": [
        { "expr": "backfill_bytes_total{type=\"json_raw\"}", "legendFormat": "JSONL (raw)", "refId": "A" },
        { "expr": "backfill_bytes_total{type=\"proto_compressed\"}", "legendFormat": "Proto (gzip)", "refId": "B" },
        { "expr": "backfill_bytes_total{type=\"proto_raw\"}", "legendFormat": "Proto (raw)", "refId": "C" },
        { "expr": "backfill_bytes_total{type=\"notepack_raw\"}", "legendFormat": "Notepack (raw)", "refId": "D" },
        { "expr": "backfill_bytes_total{type=\"notepack_compressed\"}", "legendFormat": "Notepack (gzip)", "refId": "E" }
      ],
      "title": "Data Size Comparison",
      "type": "piechart"
    },
    {
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 26 },
      "id": 103,
      "title": "Efficiency",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "max": 1,
          "min": 0,
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 0.5 }, { "color": "red", "value": 0.8 }] },
          "unit": "percentunit"
        }
      },
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 27 },
      "id": 30,
      "options": { "colorMode": "value", "graphMode": "area", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "textMode": "auto" },
      "targets": [{ "expr": "backfill_events_duplicate_total / (backfill_events_valid_total + backfill_events_duplicate_total + 1)", "legendFormat": "Duplicate Rate", "refId": "A" }],
      "title": "Duplicate Rate",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "max": 1,
          "min": 0,
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "yellow", "value": 0.01 }, { "color": "red", "value": 0.05 }] },
          "unit": "percentunit"
        }
      },
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 27 },
      "id": 31,
      "options": { "colorMode": "value", "graphMode": "area", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "textMode": "auto" },
      "targets": [{ "expr": "backfill_events_invalid_total / (backfill_events_valid_total + backfill_events_invalid_total + 1)", "legendFormat": "Invalid Rate", "refId": "A" }],
      "title": "Invalid Rate",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "thresholds": { "mode": "absolute", "steps": [{ "color": "blue", "value": null }] },
          "unit": "percent"
        }
      },
      "gridPos": { "h": 6, "w": 8, "x": 16, "y": 27 },
      "id": 32,
      "options": { "colorMode": "value", "graphMode": "none", "justifyMode": "auto", "orientation": "auto", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "textMode": "auto" },
      "targets": [{ "expr": "(1 - backfill_bytes_total{type=\"notepack_compressed\"} / (backfill_bytes_total{type=\"notepack_raw\"} + 1)) * 100", "legendFormat": "Compression Ratio", "refId": "A" }],
      "title": "Compression Savings",
      "type": "stat"
    }
  ],
  "refresh": "5s",
  "schemaVersion": 38,
  "tags": ["pensieve", "nostr", "backfill"],
  "templating": {
    "list": [
      {
        "current": { "selected": false, "text": "Prometheus", "value": "Prometheus" },
        "hide": 0,
        "includeAll": false,
        "label": "Datasource",
        "multi": false,
        "name": "datasource",
        "options": [],
        "query": "prometheus",
        "queryValue": "",
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "type": "datasource"
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Pensieve Backfill",
  "uid": "pensieve-backfill",
  "version": 1
}
